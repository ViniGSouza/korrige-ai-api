# AWS Configuration
AWS_REGION=sa-east-1
AWS_ACCOUNT_ID=your-account-id

# Anthropic Claude API
ANTHROPIC_API_KEY=your-anthropic-api-key

# OpenAI API
OPENAI_API_KEY=your-openai-api-key

# Stage
STAGE=dev

# DynamoDB Tables
MAIN_TABLE_NAME=corrige-ai-${STAGE}

# S3 Buckets
ESSAYS_BUCKET_NAME=corrige-ai-essays-${STAGE}

# SQS Queues
ESSAY_PROCESSING_QUEUE_URL=https://sqs.${AWS_REGION}.amazonaws.com/${AWS_ACCOUNT_ID}/corrige-ai-essay-processing-${STAGE}

# Cognito
USER_POOL_ID=your-user-pool-id
USER_POOL_CLIENT_ID=your-user-pool-client-id
